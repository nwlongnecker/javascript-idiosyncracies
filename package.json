{
  "name": "javascript-idisyncracies",
  "version": "0.0.1",
  "description": "This repository contains a collection of sample scripts that showcase unique and sometimes surprising JavaScript behavior. The intended audience is Java or C# devs who are not very familiar with JavaScript.",
  "main": "./dist/duck-typing/index.js",
  "scripts": {
    "build": "rm -rf -d dist && rm -rf -d out && tsc",
    "duck-typing": "yarn run build && node ./dist/1-duck-typing",
    "types-are-build-time-only": "yarn run build && node ./dist/2-types-are-build-time-only",
    "object-destructuring": "yarn run build && node ./dist/3-object-destructuring",
    "spread-operator": "yarn run build && node ./dist/4-spread-operator",
    "javascript-is-interpreted": "yarn run build && node ./dist/5-javascript-is-interpreted",
    "classes": "yarn run build && node ./dist/6-classes",
    "monkey-patching": "yarn run build && node ./dist/7-monkey-patching",
    "prototypes": "yarn run build && node ./dist/8-prototypes",
    "closures": "yarn run build && node ./dist/9-closures",
    "beware-this": "yarn run build && node ./dist/10-beware-this",
    "callbacks": "yarn run build && node ./dist/11-callbacks",
    "promises": "yarn run build && node ./dist/12-promises",
    "async-await": "yarn run build && node ./dist/13-async-await",
    "test": "jest test"
  },
  "repository": {
    "type": "git",
    "url": "git@nwlongnecker:nwlongnecker/javascript-idiosyncracies.git"
  },
  "author": "nwlongnecker",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.9",
    "typescript": "^5.4.5"
  }
}
